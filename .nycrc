{
    "include": [
      "./src/*.ts"
    ],
    "exclude": [
      "./**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
    "text-summary",
    "lcov",
    "text",
    "json",
    "cobertura"
    ],
    "watermarks": {
      "lines": [90, 100],
      "functions": [90, 100],
      "branches": [90, 100],
      "statements": [90, 100]
    }
}